{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "flair/sidler",
	"title": "Sidler",
	"category": "text",
	"icon": "text",
	"description": "Creates a marquee 2.0 style",
	"keywords": [ "glide", "slide", "marquee" ],
	"version": "0.1",
	"textdomain": "jape",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": ".line"
		}
	},
	"supports": {
		"color": {
			"background": "true",
			"gradients": "true"
		}
	},
	"editorScript": "file:./editor.js",
	"editorStyle": "file:./editor.css",
	"script": "file:./sidler.js",
	"style": "file:./sidler.css"
}